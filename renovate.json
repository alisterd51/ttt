{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "baseBranches": [
    "master"
  ],
  "commitMessageSuffix": "({{updateType}})",
  "extends": [
    "config:recommended",
    "default:semanticCommits",
    ":disableRateLimiting",
    ":rebaseStalePrs"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "test\\.ya?ml"
      ],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "(?<currentValue>.*?) # renovate: depName=(?<depName>.*?)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
